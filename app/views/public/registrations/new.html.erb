      <h2>新規会員登録</h2>
    </div>
  </div>
  <div class="row justify-content-center">
    <div class="col-12 col-xl-8">
      <%= render "public/shared/auth/signup" %>
    </div>
  </div>
  <div class="row mt-4 mb-mt-5">
    <div class="auth-page-title-box col-auto offset-0 offset-xl-2">
      <h2>既に登録済みの方</h2>
    </div>
  </div>
  <div class="row justify-content-center">
    <div class="col-auto">
      <%= render "public/shared/auth/auth_links" %>
    </div>
  </div>
</div>
